{
    "name": "hello-world-framework/routing-engine",
    "type": "project",
    "description": "Routing Engine for the HelloWorld Framework",
    "keywords": [
        "routing-engine",
        "route",
        "routing",
        "engine",
        "hello-world-framework"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.4"
    },
    "autoload": {
        "psr-4": {
            "HW\\Engines\\Route\\": "src/HW/Engines/Route/"
        }
    },
    "autoload-dev": {
